<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--绝对路径：安装目录-->
    <add key="path_deploy" value="D:\TOECProgramFiles\" />
    <!--绝对路径：备份目录-->
    <add key="path_backup" value="D:\TOECProgramFiles\AutoBackup\" />
    <!--相对路径：源压缩包-->
    <add key="path_source" value="TOEC Installers\Server\" />
    <!--相对路径：手动安装包-->
    <add key="path_manual" value="TOEC Installers\ServerBase\" />
    <!--相对路径：部署模板-->
    <add key="path_template" value="Config\Template.xml" />
    <!--数据库连接字符串-->
    <add key="db_con" value="server=localhost;database=sartas;user=sartas;pwd=sartas;port=3306;CharSet=utf8;Convert Zero Datetime=True; Allow Zero Datetime=True;Allow User Variables=True" />
    <!--启用备份-->
    <add key="enable_backup" value="false" />
    <!--删除对应进程名称的软件-->
    <add key="del_process" value="DataImportExportService,ImageAlgSvr"/>
  </appSettings>
</configuration>